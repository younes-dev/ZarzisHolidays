<!DOCTYPE html>
<html>
{# https://jsfiddle.net/nLj1uaqc/ #}
{# https://www.coordonnees-gps.fr/ #}
{# https://leafletjs.com/ #}
{# https://www.coordonnees-gps.fr/ #}
{# https://www.tutorialspoint.com/google_maps/google_maps_types.htm #}

<head>
    <link rel="stylesheet" href="https://unpkg.com/leaflet@1.3.1/dist/leaflet.css"
          integrity="sha512-Rksm5RenBEKSKFjgI3a41vrjkw4EVPlJ3+OiI65vTjIdo9brlAacEuKOiQ5OFh7cOI1bkDwLqdLw3Zg0cRJAAQ=="
          crossorigin=""/>
    <script src="https://unpkg.com/leaflet@1.3.1/dist/leaflet.js"
            integrity="sha512-/Nsx9X4HebavoBvEBuyp3I7od5tA0UzAxs+j83KgC8PU0kgB4XiK4Lfe4y4cgBtaRJQEIFCW+oC506aPT2L1zw=="
            crossorigin=""></script>
</head>

<body>
<style>
    div.leaflet-popup-content-wrapper {
        background-color: #9e9d24 !important;
        color: #1c1c1c !important;
    }
</style>


<table>
    <tr>
        <td>
            {# ############################################################################################### #}
            {# ##################################        First  Maps        ################################## #}
            {# ############################################################################################### #}

            <div id="mapDiv" style="width: 600px; height: 600px"></div>
            <script>

                //*******************
                // position we will use later
                var lat1 = 33.9954722;
                var lon1 = -6.8473962;
                //*******************
                var lat2 = 34.0174013;
                var lon2 = -6.8347757;

                var lat3 = 33.5810259;
                var lon3 = -7.6322009;


                // initialize map
                map = L.map('mapDiv').setView([lat1, lon1], 13);

                // set map tiles source
                L.tileLayer('https://tile.openstreetmap.org/{z}/{x}/{y}.png', {
                    attribution: 'Map data &copy; <a href="https://www.openstreetmap.org/">OpenStreetMap</a> contributors',
                    // minZoom: 15,
                    maxZoom: 26,
                }).addTo(map);

                // add marker to the map
                marker = L.marker([lat1, lon1]).addTo(map);
                marker2 = L.marker([lat2, lon2]).addTo(map);
                marker3 = L.marker([lat3, lon3]).addTo(map);

                // add popup to the marker
                marker.bindPopup("<b>McDonald's</b><br /> Avenue Fal Ould Oumeir, Agdal<br />Rabat Maroc").openPopup();
                marker2.bindPopup("<b>McDonald's</b><br /> Rue Prince Moulay Abdellah, 10000 Rabat,<br />10000 Rabat Maroc").openPopup();
                marker3.bindPopup("<b> Bio Ice,</b><br /> Boulevard du 9 Avril, <br />20104 Casablanca, Maroc,").openPopup();
            </script>


        </td>
        <td>

            {# ############################################################################################### #}
            {# ##################################        Second Maps        ################################## #}
            {# ############################################################################################### #}

            <iframe width="600" height="600" frameborder="0" scrolling="no" marginheight="0" marginwidth="0"
                    id="gmap_canvas"
                    src="https://maps.google.com/maps?width=900&amp;height=700&amp;hl=en&amp;q=Bio%20Ice,%20Boulevard%20du%209%20Avril%20casablanca+(Bio%20Ice,%20)&amp;t=&amp;z=18&amp;ie=UTF8&amp;iwloc=B&amp;output=embed">
            </iframe>
            <a href='http://maps-generator.com/'>embedded google maps</a>
            <script type='text/javascript'
                    src='https://embedmaps.com/google-maps-authorization/script.js?id=296ece86f7c415a52e16e9765a59d7490f8074aa'></script>


        </td>
    </tr>
</table>
</body>
</html>




































